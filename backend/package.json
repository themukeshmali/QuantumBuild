{
    "name": "quantumbuild-backend",
    "version": "1.0.0",
    "description": "Backend API for Quantum Build E-Commerce",
    "main": "server.js",
    "type": "module",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "start": "node server.js",
        "server": "nodemon server.js",
        "client": "npm start --prefix ../frontend",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "data:import": "node data/seeder.js",
        "data:destroy": "node data/seeder.js -d"
    },
    "author": "Quantum Build",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "colors": "^1.4.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-async-handler": "^1.2.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^7.6.3",
        "cors": "^2.8.5"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "nodemon": "^3.0.1"
    }
}